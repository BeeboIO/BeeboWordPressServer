{
    "name": "beeboio/beebo-wordpress-server",
    "description": "Beebo Server WordPress Plugin",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Aaron Collegeman",
            "email": "yo@aaroncollegeman.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.0",
        "beeboio/atproto-php": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeeboIO\\WordPress\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeeboIO\\WordPress\\Server\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "beebo-wordpress-server"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./lib/beeboio/atproto-php"
        }
    ]
}